{
  "name": "tauri-plugin-web-bluetooth-api",
  "version": "0.1.0",
  "author": "ParticleG <particle_g@outlook.com>",
  "description": "Tauri adapter for Web Bluetooth API",
  "type": "module",
  "types": "./dist-js/index.d.mts",
  "main": "./dist-js/index.cjs",
  "module": "./dist-js/index.mjs",
  "exports": {
    ".": {
      "types": "./dist-js/index.d.mts",
      "import": "./dist-js/index.mjs",
      "require": "./dist-js/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist-js",
    "README.md"
  ],
  "workspaces": [
    "examples/tauri-app"
  ],
  "scripts": {
    "build": "tsdown",
    "example:dev": "bun run build && bun run --cwd ./examples/tauri-app dev"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0"
  },
  "devDependencies": {
    "tsdown": "^0.17.2",
    "typescript": "^5.3.3"
  }
}
